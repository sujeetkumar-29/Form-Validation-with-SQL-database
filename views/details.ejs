<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Form Data Details</title>
  <link rel="stylesheet" href="/styles.css">
</head>
<body>
  <h1>Submitted Form Data</h1>
  
  <table border="1">
    <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Email</th>
        <th>Password</th>
      </tr>
    </thead>
    <tbody>
      <% users.forEach(user => { %>
        <tr>
          <td><%= user.id %></td>
          <td><%= user.name %></td>
          <td><%= user.email %></td>
          <td><%= user.password %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>

  <a href="/">Back to Form</a>
</body>
</html>
